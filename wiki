#!/usr/bin/env perl6
use v6;

use November;
use November::CGI;
use November::Config;
use Text::Markup::Wiki::MediaWiki;

my $c = November::Config.new(
    markup => Text::Markup::Wiki::MediaWiki.new(),
    skin   => 'CleanAndSoft'
);
my November $wiki = November.new(
    config => $c,
);
my $cgi = November::CGI.new;
$wiki.handle_request($cgi);
